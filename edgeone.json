{
    "name": "example-app",
    "buildCommand": "next build",
    "installCommand": "npm install",
    "outputDirectory": "./build",
    "nodeVersion": "22.11.0",
   
    "redirects": [
      {
        "source": "/articles/:id",
        "destination": "/news-articles/:id",
        "statusCode": 301
      },
      {
        "source": "/old-path",
        "destination": "/new-path",
        "statusCode": 302
      }
    ],
    "rewrites": [
      {
        "source": "/assets/*",
        "destination": "/assets-new/:splat"
      }
    ],
    "headers": [
      {
        "source": "/*",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "Cache-Control",
            "value": "max-age=7200"
          }
        ]
      },
      {
        "source": "/assets/*",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  }
